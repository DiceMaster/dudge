<?xml version="1.0" encoding="UTF-8"?>

<ejb-jar xmlns = "http://java.sun.com/xml/ns/javaee" 
         version = "3.0" 
         xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance" 
         xsi:schemaLocation = "http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd">
<enterprise-beans>
	<session>
		<ejb-name>SlaveBean</ejb-name>
		<ejb-class>dudge.slave.SlaveBean</ejb-class>
		<env-entry>
			<description>Languages supported by slave.</description>
			<env-entry-name>slaveLanguages</env-entry-name>
			<env-entry-type>java.lang.String</env-entry-type>
			<env-entry-value>gcc fpc java</env-entry-value>
		</env-entry>
		<env-entry>
			<description>Use privelege drop during solution launch.</description>
			<env-entry-name>launcherUsePrivilegeDrop</env-entry-name>
			<env-entry-type>java.lang.Boolean</env-entry-type>
			<env-entry-value>true</env-entry-value>
		</env-entry>
		<env-entry>
			<description>Username for privelege drop during solution launch.</description>
			<env-entry-name>launcherUsername</env-entry-name>
			<env-entry-type>java.lang.String</env-entry-type>
			<env-entry-value>dudge</env-entry-value>
		</env-entry>
		<env-entry>
			<description>Password for privelege drop during solution launch.</description>
			<env-entry-name>launcherPassword</env-entry-name>
			<env-entry-type>java.lang.String</env-entry-type>
			<env-entry-value></env-entry-value>
		</env-entry>
		<env-entry>
			<description>Domain for privelege drop during solution launch.</description>
			<env-entry-name>launcherDomain</env-entry-name>
			<env-entry-type>java.lang.String</env-entry-type>
			<env-entry-value>.</env-entry-value>
		</env-entry>
	</session>
</enterprise-beans>
</ejb-jar>
